<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="600.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.firstproj02.view.DiscountController">
   <children>
      <VBox layoutX="150.0" layoutY="175.0" prefHeight="250.0" prefWidth="300.0">
         <children>
            <Button fx:id="customerDiscountButton" mnemonicParsing="false" onAction="#customerDiscountButton" prefHeight="100.0" prefWidth="300.0" styleClass="light-btn" stylesheets="@../styles/styles.css" text="Customer Disscount">
               <font>
                  <Font name="Gabriola" size="30.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="productDiscountButton" mnemonicParsing="false" onAction="#productDiscountButton" prefHeight="100.0" prefWidth="300.0" styleClass="light-btn" stylesheets="@../styles/styles.css" text="Product Discount">
               <font>
                  <Font name="Gabriola" size="30.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
